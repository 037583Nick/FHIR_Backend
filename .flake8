[flake8]
max-line-length = 200
max-complexity = 30
exclude = 
    trtis/,
    __pycache__,
    *.pyc,
    venv,
    env,
    .venv,
    .env,
    .git,
    .tox,
    dist,
    build

ignore = 
    W293,
    W291,
    W391,
    W292,
    W503,
    F401,
    F841,
    E203,
    E251,
    E226,
    E231,
    E261,
    E275,
    E301,
    E302,
    E303,
    E305,
    E402,
    E501,
    E712,
    E722,
    E123,
    E126,
    E128,
    F541,
    F811,
    C901
    
per-file-ignores =
    trtis/*.py:E,F,W,C
    */__init__.py:F401
